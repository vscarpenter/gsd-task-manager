{
  "name": "gsd-taskmanager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf .next out",
    "prebuild": "npm run clean",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "export": "next export",
    "deploy:s3": "aws s3 sync out/ s3://gsd.vinny.dev/ --delete --cache-control 'public,max-age=31536000,immutable'",
    "deploy:cf": "aws cloudfront create-invalidation --distribution-id E1T6GDX0TQEP94 --paths '/*'",
    "deploy": "npm run build && npm run deploy:s3 && npm run deploy:cf"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "clsx": "^2.1.1",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^1.1.7",
    "lucide-react": "^0.370.0",
    "nanoid": "^5.1.6",
    "next": "15.5.4",
    "next-themes": "^0.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.19",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.4",
    "jsdom": "^24.1.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "license": "MIT"
}
