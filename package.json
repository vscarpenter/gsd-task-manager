{
  "name": "gsd-taskmanager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf .next out",
    "prebuild": "npm run clean",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "export": "next export",
    "deploy:s3": "aws s3 sync out/ s3://gsd.vinny.dev/ --delete --cache-control 'public,max-age=31536000,immutable'",
    "deploy:cf": "aws cloudfront create-invalidation --distribution-id E1T6GDX0TQEP94 --paths '/*'",
    "deploy": "npm run build && npm run deploy:s3 && npm run deploy:cf"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "clsx": "^2.1.0",
    "dexie": "^4.0.4",
    "dexie-react-hooks": "^1.1.7",
    "lucide-react": "^0.370.0",
    "nanoid": "^4.0.2",
    "next": "15.0.0-canary.113",
    "next-themes": "^0.3.0",
    "react": "rc",
    "react-dom": "rc",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0-canary.113",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "license": "MIT"
}
