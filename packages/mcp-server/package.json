{
  "name": "gsd-mcp-server",
  "version": "0.6.0",
  "description": "MCP server for GSD Task Manager - full task management with create/update/delete, analytics, interactive setup, and validation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gsd-mcp-server": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "task-manager",
    "productivity",
    "claude",
    "claude-desktop",
    "eisenhower-matrix",
    "encryption",
    "privacy"
  ],
  "author": "Vinny Carpenter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vscarpenter/gsd-taskmanager.git",
    "directory": "packages/mcp-server"
  },
  "homepage": "https://github.com/vscarpenter/gsd-taskmanager#readme",
  "bugs": {
    "url": "https://github.com/vscarpenter/gsd-taskmanager/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@vitest/ui": "^4.0.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
